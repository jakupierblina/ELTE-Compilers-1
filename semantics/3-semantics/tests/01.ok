program oszto
  integer a;
  integer i;
  integer oszto;
  boolean vanoszto;
  time t;
begin
  read(a);
  vanoszto := false;
  i := 2;
  t := 12:23;
  while not vanoszto and i < a and t=10:00 do
    if a mod i = 0 then
      vanoszto := true;
      oszto := i;
    endif
    i := i+1;
  done
  if vanoszto then
    write(vanoszto);
    write(oszto);
  else
    write(vanoszto);
  endif
  write(hour(t));
end
