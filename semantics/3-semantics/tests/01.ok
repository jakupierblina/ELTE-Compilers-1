program oszto
  integer a;
  integer i;
  integer oszto;
  boolean vanoszto;
begin
  read(a);
  vanoszto := false;
  i := 2;
  target:
  while not vanoszto and i < a do
    if a mod i = 0 then
      vanoszto := true;
      oszto := i;
    endif
    i := i+1;
  done
  target:
  if vanoszto then
    write(vanoszto);
    write(oszto);
  else
    write(vanoszto);
    goto target1
  endif
end
